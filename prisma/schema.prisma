datasource db {
  provider = "postgresql"
  url      = env.DATABASE_URL
}

generator client {
  provider = "prisma-client-js"
}

model BucketItem {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  category    String
  location    String
  coordinates String
  image       String?
  likes       Int      @default(0)
  enabled     Boolean  @default(true)
  order       Int      @default(0)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}